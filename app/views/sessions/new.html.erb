<%= provide(:title,"log in") %>
<h1>Log in</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url:login_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "login" %>
    <% end %>
    <p> new user? <%= link_to("sign up now" ,users_new_path)  %> </p>
  </div>
</div>